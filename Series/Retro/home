<item>
<title>[COLOR red]Los Invasores[/COLOR]</title>
<link>$doregex[makelist]</link>
<regex>
  <name>makelist</name>
  <listrepeat><![CDATA[
        <title>[COLOR blue][makelist.param2][/COLOR]</title>
        <link>$doregex[getUrl]</link>
        <thumbnail></thumbnail>
	<fanart></fanart>
  ]]></listrepeat>
<expres><![CDATA[class="name".*href="\/([^"]*)".+?>(.*?)<[\w\W\s]*?action="\/([^"]*)".+?value="(.*?)".*Token.+?value="(.*?)"]]></expres>
<page>http://copiapop.com/Cuspide/los-invasores-serie-54899/gallery,1,2</page>
<cookieJar></cookieJar>
</regex>

<regex>
<name>getUrl</name>
<expres>DownloadUrl":"([^"]+)<expres>
<page>http://copiapop.com/[makelist.param3]</page>
<rawpost>fileId=[makelist.param4]&__RequestVerificationToken=[makelist.param5]</rawpost>
<referer>http://copiapop.com/[makelist.param1]</referer>
<connection>keep-alive</connection>
<accept>*/*</accept>
<X-Req>XMLHttpRequest</X-Req>
<cookieJar></cookieJar>
</regex>
<thumbnail></thumbnail>
